package com.demo;

import java.math.BigDecimal;
import java.util.*;

public class ProductCollection {

	private List<Product> products= new ArrayList<Product>();

	public List<Product> getProducts() {
		products.add(new Product(121, "laptop", new BigDecimal(56000), true));
		products.add(new Product(121, "mouse", new BigDecimal(500), true));
		return products;
	}

	
	
}
